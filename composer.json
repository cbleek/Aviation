{
    "type": "yawik-module",
    "name": "yawik/aviation",
    "description": "aviation modifies the startpage of a yawik installation.",
    "license": "MIT",
    "authors": [
        {
            "name": "Carsten Bleek",
            "email": "bleek@cross-solution.de"
        }
    ],
    "support": {
        "source": "https://github.com/DCAviation/Aviation"
    },
    "keywords": [
        "yawik",
        "yawik-module"
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "yawik/core": "^0.35",
        "yawik/jobs": "^0.35",
        "yawik/auth": "^0.35",
        "yawik/applications": "^0.35",
        "yawik/settings": "^0.35",
        "yawik/geo": "^1.1",
        "yawik/cv": "^0.35",
        "yawik/organizations": "^0.35",
        "yawik/pdf": "^0.35",
        "yawik/composer-plugin": "^3.0",
        "laminas/laminas-dependency-plugin": "^2.0  "
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "yawik/behat": "^0.33"
    },
    "autoload": {
        "psr-4": {
            "Aviation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": "test/"
        }
    },
    "config": {
        "platform": {
            "ext-mongo": "1.7"
        },
        "process-timeout": 0
    },
    "scripts": {
        "auto-scripts": [
            "npm install"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "serve": "php -S 0.0.0.0:8000 -t test/sandbox/public test/sandbox/public/router.php",
        "start-selenium": "./vendor/bin/selenium-server-standalone -p 4444 -Dwebdriver.chrome.driver=./vendor/bin/chromedriver",
        "test": "phpunit"
    },
    "extra": {
        "zf": {
            "module": "Aviation"
        },
        "branch-alias": {
            "dev-develop": "0.32-dev"
        }
    }
}
